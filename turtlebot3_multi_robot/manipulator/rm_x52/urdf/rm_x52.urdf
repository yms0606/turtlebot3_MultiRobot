            </geometry>
            <material name="red"/>
        </visual>
    </link>






    <!-- Base to Joint1 -->
    <joint name="base_joint1" type="revolute">
        <parent link="base_link"/>
        <child link="joint1"/>
        <origin xyz="0 0 0" rpy="1.57 0 0"/> <!-- 6mm -->
        <axis xyz="0 1 0"/> <!-- z축 회전 -->
        <limit lower="-3.14159" upper="3.14159" effort="50.0" velocity="1.0"/>
    </joint>

    <!-- Joint1 to Link2 -->
    <joint name="joint1_link2" type="fixed">
        <parent link="joint1"/>
        <child link="link2"/>
        <origin xyz="0 0.055 0" rpy="0 0 0"/>
    </joint>

    <!-- Link2 to Joint3 -->
    <joint name="link2_joint3" type="fixed">
        <parent link="link2"/>
        <child link="joint3"/>
        <origin xyz="0 0.021 0" rpy="0 0 0"/>
    </joint>

    <!-- Joint3 to Link4 -->
    <joint name="joint3_link4" type="revolute">
        <parent link="joint3"/>
        <child link="link4"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1.57" upper="1.57" effort="50.0" velocity="1.0"/>
    </joint>

    <!-- Link4 to Link5 -->
    <joint name="link4_link5" type="fixed">
        <parent link="link4"/>
        <child link="link5"/>
        <origin xyz="0 0.028 0" rpy="0 0 0"/>
    </joint>

    <!-- Link5 to Link6 -->
    <joint name="link5_link6" type="fixed">
        <parent link="link5"/>
        <child link="link6"/>
        <origin xyz="0 0.099 0" rpy="0 0 0"/>
    </joint>

        <!-- Link6 to Joint7 -->
    <joint name="link6_joint7" type="fixed">
        <parent link="link6"/>
        <child link="joint7"/>
        <origin xyz="0.024 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Joint7 to Link8 -->
    <joint name="joint7_link8" type="revolute">
        <parent link="joint7"/>
        <child link="link8"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1.57" upper="1.57" effort="50.0" velocity="1.0"/>
    </joint>

    <!-- Link8 to Link9 -->
    <joint name="link8_link9" type="fixed">
        <parent link="link8"/>
        <child link="link9"/>
        <origin xyz="0.028 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Link9 to Link10 -->
    <joint name="link9_link10" type="fixed">
        <parent link="link9"/>
        <child link="link10"/>
        <origin xyz="0.075 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Link10 to Joint11 -->
    <joint name="link10_joint11" type="fixed">
        <parent link="link10"/>
        <child link="joint11"/>
        <origin xyz="0.021 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Joint11 to Link12 -->
    <joint name="joint11_link12" type="revolute">
        <parent link="joint11"/>
        <child link="link12"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="-1.57" upper="1.57" effort="50.0" velocity="1.0"/>
    </joint>


    <!-- Link12 to Joint13 -->
    <joint name="link12_joint13" type="fixed">
        <parent link="link12"/>
        <child link="joint13"/>
        <origin xyz="0.045 0 0" rpy="0 0 0"/>
    </joint>



    <!--Gripper 1 -->
    <joint name="joint13_lbody1" type="fixed">
        <parent link="joint13"/>
        <child link="left_finger1"/>
        <origin xyz="0.022 0 0.002" rpy="0 0 0"/>
    </joint>


    <joint name="joint13_rbody1" type="fixed">
        <parent link="joint13"/>
        <child link="right_finger1"/>
        <origin xyz="0.022 0 -0.002" rpy="0 0 0"/>
    </joint>

   <!--Gripper 2 -->
    <joint name="lbody1_lbody2" type="prismatic">
        <parent link="left_finger1"/>
        <child link="left_finger2"/>
        <origin xyz="0 0 -0.015" rpy="0 0 0"/>
        <axis xyz="0 0 1"/> <!-- z축 방향 -->
        <limit lower="0.0" upper="0" effort="50.0" velocity="3.0"/>
    </joint>

    <joint name="rbody1_rbody2" type="prismatic">
        <parent link="right_finger1"/>
        <child link="right_finger2"/>
        <origin xyz="0 0 0.015" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
        <limit lower="0.0" upper="0.03" effort="50.0" velocity="3.0"/>
        <mimic joint="lbody1_lbody2" multiplier="-1.0"/> <!-- gripper_right의 반대 방향으로 이동 -->
    </joint>



    <!--Gripper 3 -->
    <joint name="lbody2_lbody3" type="fixed">
        <parent link="left_finger2"/>
        <child link="left_finger3"/>
        <origin xyz="0.014 0 0.013" rpy="0 0 0"/>
    </joint>


   <joint name="rbody2_rbody3" type="fixed">
        <parent link="right_finger2"/>
        <child link="right_finger3"/>
        <origin xyz="0.014 0 -0.013" rpy="0 0 0"/>
    </joint>

    <!--Gripper 4 -->
    <joint name="lbody3_lbody4" type="fixed">
        <parent link="left_finger3"/>
        <child link="left_finger4"/>
        <origin xyz="0.045 0 0.002" rpy="0 0 0"/>
    </joint>


   <joint name="rbody3_rbody4" type="fixed">
        <parent link="right_finger3"/>
        <child link="right_finger4"/>
        <origin xyz="0.045 0 -0.002" rpy="0 0 0"/>
    </joint>

    <!--Gripper center -->
    <joint name="center" type="fixed">
        <parent link="left_finger1"/>
        <child link="finger_center"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>





</robot>
